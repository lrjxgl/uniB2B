
.b{
	font-weight: bolder;
}
.aBox{
	background-color: #fff;
	margin-bottom: 5px;
	padding: 10px;
}
.aBox-line{
	height: 10px;
	background-color: #eee;
	width: 1px;
	margin: 0 8px;
}
.aBox-btn{
	width: 80rpx;
	height: 60rpx;
	line-height: 60rpx;
	text-align: center;
	font-size: 12px;
	background-color: #ffd356;
	color: #333;
	border-radius: 3px;
	cursor: pointer;
}
.aBox-btnc{
	border: 1px solid #eee;
	padding: 3px 5px;
	color: #fb4e44;
	margin-right: 5px;
	border-radius: 5px;
	font-size: 12px;
}
.apBox{
	width: 33.3%;
	padding: 5px;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.apBox-img{
	width: 80%;
	margin: 0 auto;
	margin-bottom: 3px;
}
.apBox-title{
	font-size: 12px;
	white-space: nowrap;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	margin-bottom: 5px;
	color: #323232;
}

